{
  "name": "boilerplate-react-universal",
  "version": "1.0.0",
  "license": "LICENSE",
  "engines": {
    "node": "8.6.0"
  },
  "scripts": {
    "clean:build:public": "rimraf ./build/public",
    "clean:build:src": "rimraf ./build/src",
    "clean": "rimraf ./build",
    "compile:webpack:dll": "BABEL_ENV=production && NODE_ENV=production && node_modules/.bin/webpack --config=config/webpack/client/dll.js",
    "compile:webpack:client": "BABEL_ENV=production && NODE_ENV=production && node_modules/.bin/webpack --config=config/webpack/client/index.js -p --cache=true --colors --env.file=false",
    "compile:webpack:server": "BABEL_ENV=production && NODE_ENV=production && node_modules/.bin/webpack --config=config/webpack/server/index.js -p --cache=true --colors",
    "compile:webpack": "BABEL_ENV=production && NODE_ENV=production && npm run compile:webpack:client && npm run compile:webpack:server",
    "compile:babel": "BABEL_ENV=production && NODE_ENV=production && node_modules/.bin/babel src --out-dir build/src --presets env,react --plugins dynamic-import-webpack,transform-class-properties --compact true --no-comments --no-highlight-code -D",
    "compile": "npm run clean:dist && npm run compile:webpack:dll && npm run compile:webpack && npm run compile:babel",
    "paths:add": "node config/utils/writePaths.js",
    "paths:del": "rimraf config/utils/paths.json",
    "server:start": "NODE_ENV=production && node build/src/server/index.js",
    "build": "npm run clean && npm run paths:del && npm run paths:add && npm run compile:webpack:dll && npm run compile:webpack:client && npm run compile:webpack:server && npm run compile:babel && npm run start:server",
    "start": "node ./config/utils/CLI.js"
  },
  "dependencies": {
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.17.2",
    "bundle-loader": "^0.5.5",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "fs-scanner": "^1.0.0",
    "immutable": "^3.8.1",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "pug": "^2.0.0-rc.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.1.1",
    "serve-favicons": "^2.1.5",
    "stylus": "^0.54.5",
    "superagent": "^3.5.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-resolver": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "colors": "^1.1.2",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.3",
    "ejs-loader": "^0.3.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.11.1",
    "glob": "^7.1.2",
    "glob-all": "^3.1.0",
    "html-loader": "^0.4.5",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-inline-source-plugin": "0.0.8",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "nib": "^1.1.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "postcss": "^6.0.1",
    "postcss-loader": "^2.0.5",
    "postcss-smart-import": "^0.7.2",
    "precss": "^1.4.0",
    "pug-loader": "^2.3.0",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "^2.4.5",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-node-externals": "^1.6.0"
  }
}
